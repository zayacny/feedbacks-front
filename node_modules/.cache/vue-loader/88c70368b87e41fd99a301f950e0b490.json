include position-absolute($top: 0, $right: 0);\n    border-top-right-radius: 50% 105px;\n  }\n  \n  .teapot-body-inner {\n    \n    //inners styles\n    &-1 {\n      background-color: $teapot-body-bright;\n      @include position-absolute($left: 0, $right: 0, $bottom: 0);\n      width: 130px;\n      height: 100%;\n      margin: auto;\n      border-top-left-radius: 50% 105px;\n      border-top-right-radius: 50% 105px;\n      z-index: 5;\n      \n      &:before {\n        content: \"\";\n        display: block;\n        background-color: darken($teapot-body-bright, 3%);\n        width: 65px;\n        height: 100%;\n        @include position-absolute($top: 0, $right: 0);\n        border-top-right-radius: 50% 105px;\n      }\n      \n        //bleak styles\n      .bleak {\n        background-color: $bleak;\n        border-radius: 50%;\n\n        &-1 {\n          width: 7px;\n          height: 12px;\n          @include position-absolute($top: calc(50% - 20px), $left: 13px);\n          transform: rotate(16deg);\n        }\n        \n        &-2 {\n          width: 4px;\n          height: 4px;\n          @include position-absolute($top: calc(50% - 30px), $left: 20px);\n        }\n      }\n    }\n    \n    &-2 {\n      background-color: $teapot-body;\n      @include position-absolute($left: 0, $right: 0, $bottom: 0);\n      width: 92px;\n      height: 100%;\n      margin: auto;\n      border-top-left-radius: 50% 105px;\n      border-top-right-radius: 50% 105px;\n      z-index: 8;\n      \n      &:before {\n        content: \"\";\n        display: block;\n        background-color: darken($teapot-body, 5%);\n        width: 43px;\n        height: 100%;\n        @include position-absolute($top: 0, $right: 0);\n        border-top-right-radius: 50% 105px;\n      }\n    }\n  }\n}\n\n//teapot top styles\n.teapot-top {\n  width: 74px;\n  height: 12px;\n  background-color: $teapot-top;\n  @include position-absolute($top: -2px, $left: 0, $right: 0);\n  margin: auto;\n  z-index: 15;\n  border-radius: 20px 20px 3px 3px;\n  \n  &:before {\n    content: \"\";\n    display: block;\n    width: 35px;\n    height: 100%;\n    @include position-absolute($top: 0px, $right: -1px);\n    background-color: darken($teapot-top, 5%);\n    border-radius: 0px 20px 3px 3px;\n  }\n  \n  &:after {\n    content: \"\";\n    display: block;\n    width: 16px;\n    height: 12px;\n    background-color: darken($teapot-top, 5%);\n    @include position-absolute($top: -11px, $left: 0, $right: 0);\n    margin: auto;\n    border-radius: 50% 50% 0 0;\n  }\n}\n\n//teapot nose styles\n.teapot-nose {\n  @include position-absolute($top: 52px, $right: 8px);\n  background-color: $teapot-nose;\n  width: 60px;\n  height: 27px;\n  transform: rotate(-40deg);\n  z-index: -1;\n  border-top-right-radius: 50% 50px;\n  border-bottom-right-radius: 50% 50px;\n  overflow: hidden;\n  \n  &:before {\n    content: \"\";\n    width: 100%;\n    height: 45%;\n    display: block;\n    background-color: darken($teapot-nose, 10%);\n    @include position-absolute($bottom: 0, $left: 0);\n  }\n  \n  &:after {\n    content: \"\";\n    display: block;\n    width: 12px;\n    height: 100%;\n    background-color: $teapot-top;\n    @include position-absolute($top: 0, $right: 0);\n  }\n}\n\n//teapot handle styles {\n.teapot-handle {\n  width: 106px;\n  height: 80px;\n  @include position-absolute($top: 0, $right: 29px);\n  \n  &:before {\n    content: \"\";\n    display: block;\n    background-color: $teapot-handle;\n    height: 100%;\n    width: 15px;\n    @include position-absolute($top: 0, $right: 0px);\n    transform-origin: center top;\n    transform: rotate(26deg);\n    border-radius: 7px\n  }\n  \n  &:after {\n    content: \"\";\n    display: block;\n    background-color: $teapot-handle;\n    height: 15px;\n    width: 100%;\n    @include position-absolute($top: -3px, $left: -3px);\n    border-radius: 7px\n  }\n}\n\n//teapot bubble styles\n@keyframes bubble {\n  0% {\n    transform: scale(0.3);\n    @include position-absolute($top: 16px, $right: -28px);\n  }\n  \n  25% {\n    transform: scale(0.5);\n    @include position-absolute($top: 6px, $right: -45px);\n  }\n  \n  50% {\n    transform: scale(0.8);\n    @include position-absolute($top: 0px, $right: -65px);\n  }\n  \n  100% {\n    transform: scale(0.8);\n    @include position-absolute($top: 0px, $right: -65px);\n  }\n}\n\n.teapot-bubble {\n  background-color: darken(#EAE8FF, 5%);\n  width: 40px;\n  height: 40px;\n  border: 2px solid #1f1f1f;\n  border-radius: 50%;\n  @include position-absolute($top: 16px, $right: -28px);\n  transform: scale(0.3);\n  animation: bubble 2s linear 0s infinite;\n  \n  &:before {\n    content: \"\";\n    display: block;\n    width: 100%;\n    height: 100%;\n    background-color: inherit;\n    @include position-absolute($top: 13px, $left: -11px);\n    border-radius: 50%;\n    border: 2px solid #1f1f1f;\n    border-right: none;\n    border-top: none;\n  }\n  \n  &:after {\n    content: \"\";\n    display: block;\n    width: 100%;\n    height: 100%;\n    background-color: inherit;\n    @include position-absolute($top: -9px, $left: -20px);\n    border-radius: 50%;\n    border: 2px solid #1f1f1f;\n    border-bottom: none;\n    border-right: none;\n  }\n}\n</style>"]}]}